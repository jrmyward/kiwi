<div class="event-block">
  <h3><%= date.strftime('%-d %B, %Y - %A') %></h3>
  <div class="events">
    <% events.each do |event| %>
      <%= render partial: 'event_summary', locals: { event: event } %>
    <% end %>
  </div>
  <div class="button-container">
    <button class='btn btn-more'>Show More</button>
  </div>
</div>
