<h5 class="filter-title">Your country<sup>?</sup> is set to:</h5>
<select class="form-control" name="country">
  <% @all_countries.each do |country| %>
    <option value="<%=country.code%>" <%= (country.code == @country) ? 'selected' : '' %>><%=country.en_name%></option>
  <% end %>
</select>
<div class="clearfix"></div>
