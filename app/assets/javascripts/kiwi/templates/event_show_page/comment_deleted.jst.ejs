<div class="up-vote">
  <i class="fa fa-caret-up"></i>
  <i class="fa fa-caret-down"></i>
</div>
<div class="user-comment deleted">
  <div class="info">
    <span class="username"> <%= username %> </span>
    <div class="tools">
      <span class="upvotes"><%= upvotes %></span> upvote(s) - <span class="reply">Reply</span></div>
    </div>
  <div class="comment-text">Deleted by <%= deleter %></div>
</div>
<div class="clearfix"></div>
<div class="nested-comments">
  <div class="replies-region"></div>
  <div class="replybox-region"></div>
</div>
<div class="clearfix"></div>
